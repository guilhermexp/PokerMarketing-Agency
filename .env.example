# Google Gemini API
GEMINI_API_KEY=your_gemini_api_key

# Fal.ai (Sora video generation)
FAL_KEY=your_fal_key


# Vercel Blob Storage
BLOB_READ_WRITE_TOKEN=your_blob_token

# Neon PostgreSQL Database
DATABASE_URL=postgresql://user:password@your-project.neon.tech/neondb?sslmode=require

# Clerk Authentication
VITE_CLERK_PUBLISHABLE_KEY=pk_test_YOUR_PUBLISHABLE_KEY
CLERK_SECRET_KEY=sk_test_YOUR_SECRET_KEY

# OpenRouter (for GPT-5.2, Grok 4.1 and other models)
OPENROUTER_API_KEY=your_openrouter_api_key

# Upstash Redis (for rate limiting and caching)
UPSTASH_REDIS_REST_URL=https://your-redis.upstash.io
UPSTASH_REDIS_REST_TOKEN=your_upstash_token


# Redis (for BullMQ job processing on Railway)
REDIS_URL=redis://default:password@your-redis.railway.internal:6379
