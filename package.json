{
  "name": "poker-marketing-agency",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently -n api,vite -c cyan,green \"node server/dev-api.mjs\" \"vite\"",
    "dev:local": "bun run dev:redis && concurrently -n redis,api,vite -c red,cyan,green \"docker compose logs -f redis\" \"bun run dev:api\" \"bun run dev:vite\"",
    "dev:api": "node server/dev-api.mjs",
    "dev:vite": "vite",
    "dev:redis": "docker compose down redis 2>/dev/null; docker compose up -d redis",
    "dev:stop": "docker compose down",
    "build": "vite build",
    "preview": "vite preview",
    "start": "node server/index.mjs",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@clerk/backend": "^2.29.0",
    "@clerk/clerk-react": "^5.59.2",
    "@clerk/express": "^1.7.60",
    "@fal-ai/client": "^1.8.0",
    "@ffmpeg/ffmpeg": "^0.12.15",
    "@ffmpeg/util": "^0.12.2",
    "@google/genai": "^1.17.0",
    "@neondatabase/serverless": "^1.0.2",
    "@openrouter/sdk": "^0.3.10",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@upstash/qstash": "^2.8.4",
    "@upstash/ratelimit": "^2.0.7",
    "@upstash/redis": "^1.36.0",
    "@vercel/blob": "^2.0.0",
    "bullmq": "^5.66.4",
    "cloudinary": "^2.8.0",
    "framer-motion": "^12.24.10",
    "ioredis": "^5.8.2",
    "lucide-react": "^0.562.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-dropzone": "^14.3.8",
    "react-router-dom": "^7.11.0",
    "recharts": "^3.6.0",
    "swr": "^2.3.8",
    "tesseract.js": "^7.0.0",
    "xlsx": "0.18.5",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@vercel/node": "^3.0.1",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.23",
    "concurrently": "^9.2.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "jsdom": "^27.4.0",
    "pg": "^8.16.3",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vite-plugin-pwa": "^1.2.0",
    "vite-plugin-top-level-await": "^1.6.0",
    "vite-plugin-wasm": "^3.5.0",
    "vitest": "^4.0.17"
  }
