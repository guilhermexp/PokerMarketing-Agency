{
  "name": "poker-marketing-agency",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "postinstall": "node scripts/ensure-sharp-libvips-link.mjs",
    "dev": "concurrently -n api,vite -c cyan,green \"bun run dev:api\" \"sleep 3 && vite\"",
    "bun:dev": "bun run dev",
    "dev:local": "bun run dev:redis && concurrently -n redis,api,vite -c red,cyan,green \"docker compose logs -f redis\" \"bun run dev:api\" \"bun run dev:vite\"",
    "dev:api": "node server/dev-api.mjs",
    "dev:api:debug": "node --trace-uncaught --trace-warnings server/dev-api.mjs",
    "dev:vite": "vite",
    "dev:redis": "docker compose down redis 2>/dev/null; docker compose up -d redis",
    "dev:stop": "docker compose down",
    "build": "vite build",
    "preview": "vite preview",
    "start": "node server/index.mjs",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit"
  },
